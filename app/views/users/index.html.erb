<h1>Users login</h1>

<%= form_with html: { autocomplete: "off" } do |form| %>
  <%= form.label :query, "Enter your login:" %>
  <%= form.text_field :query, placeholder: "login" %>
  <%= form.submit "Search", data: { disable_with: false } %>
<% end %>

<h2 id="name"></h2>
<ul id="list"></ul>
<p id="error" hidden></p>

<%= javascript_pack_tag 'form' %>
